<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="anchorPane" prefHeight="208.0" prefWidth="211.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Server">
    <children>
        <TextField fx:id="filePath" layoutX="27.0" layoutY="26.0" prefHeight="25.0" prefWidth="157.0" promptText="File Path" />
        <Button layoutX="27.0" layoutY="108.0" mnemonicParsing="false" onAction="#selectFile" text="Browse File" />
        <Button layoutX="27.0" layoutY="146.0" mnemonicParsing="false" onAction="#back" text="Back" />
        <Button layoutX="106.0" layoutY="108.0" mnemonicParsing="false" onAction="#send" prefHeight="25.0" prefWidth="76.0" text="Send" />
        <Label fx:id="label" layoutX="89.0" layoutY="150.0" prefHeight="17.0" prefWidth="92.0" />
    </children>
</AnchorPane>
