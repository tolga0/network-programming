<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="anchorPane" prefHeight="200.0" prefWidth="222.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Sender">
   <children>
      <TextField fx:id="filePath" layoutX="27.0" layoutY="26.0" prefHeight="25.0" prefWidth="157.0" promptText="File Path" />
      <Button layoutX="27.0" layoutY="108.0" mnemonicParsing="false" onAction="#selectFile" text="Browse File" />
      <Button layoutX="27.0" layoutY="146.0" mnemonicParsing="false" onAction="#back" text="Back" />
      <TextField fx:id="receiverIp" layoutX="27.0" layoutY="73.0" prefHeight="25.0" prefWidth="157.0" promptText="IP" />
      <Button layoutX="106.0" layoutY="108.0" mnemonicParsing="false" onAction="#sendFile" prefHeight="25.0" prefWidth="76.0" text="Send" />
      <Label fx:id="label" layoutX="89.0" layoutY="150.0" prefHeight="17.0" prefWidth="92.0" />
   </children>
</AnchorPane>
